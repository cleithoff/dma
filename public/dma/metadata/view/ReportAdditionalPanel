                                    "                    align: 'left',\r",
                                    "                    text: field.name,\r",
                                    "                    xtype: 'gridcolumn'\r",
                                    "                });\r",
                                    "                fields.push({\r",
                                    "                    name: field.name,\r",
                                    "                    type: 'string',\r",
                                    "                });                    \r",
                                    "                break;\r",
                                    "                default:\r",
                                    "                columns.push({\r",
                                    "                    dataIndex: field.name,\r",
                                    "                    align: 'left',\r",
                                    "                    text: field.name,\r",
                                    "                    xtype: 'gridcolumn'\r",
                                    "                });\r",
